<%= form_for @playlist do |f| %>
  <div>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div>
    Select Videos: <br>
    <%= f.collection_check_boxes :video_ids, Video.all, :id, :title do |box|%>
      <%= box.check_box %> 
      <%= box.label %><br>
    <% end %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
